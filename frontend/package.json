{
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "author": "",
  "babel": {
    "env": {
      "development": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "production": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "test": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.28",
    "apollo-client": "^2.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.0.0-alpha.27",
    "downshift": "^2.2.3",
    "draft-convert": "^2.1.4",
    "draft-js": "^0.10.5",
    "draft-js-import-html": "^1.3.3",
    "draftjs-to-html": "^0.8.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "font-awesome": "^4.7.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "html-to-draftjs": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "lodash.debounce": "^4.0.8",
    "next": "^7.0.3",
    "next-with-apollo": "^3.4.0",
    "nprogress": "^0.2.0",
    "paypal-rest-sdk": "^1.8.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.5.3",
    "react-dom": "^16.8.6",
    "react-draft-wysiwyg": "^1.13.2",
    "react-markdown": "^4.0.6",
    "react-quill": "^1.3.3",
    "react-rating": "^1.6.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.8.0",
    "styled-components": "^3.4.10",
    "video-react": "^0.13.6",
    "waait": "^1.0.4"
  },
  "description": "Picus",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.0",
    "enzyme-to-json": "^3.3.5",
    "graphql-tools": "^4.0.4",
    "jest": "^24.7.1",
    "jest-transform-graphql": "^2.1.0"
  },
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "license": "ISC",
  "main": "index.js",
  "name": "picus-frontend",
  "scripts": {
    "build": "next build",
    "dev": "next -p 7777",
    "heroku-postbuild": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch"
  },
  "version": "1.0.0"
}