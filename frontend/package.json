{
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "author": "",
  "babel": {
    "env": {
      "development": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "production": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "test": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
