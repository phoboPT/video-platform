{
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "author": "",
  "babel": {
    "env": {
      "development": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "production": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "next/babel"
        ]
      },
      "test": {
        "plugins": [
          [
            "styled-components",
            {
              "displayName": true,
              "ssr": true
            }
          ]
        ],
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.7.1",
    "date-fns": "^2.0.0-alpha.7",
    "downshift": "^2.2.3",
    "draft-convert": "^2.1.4",
    "draft-js": "^0.10.5",
    "draft-js-import-html": "^1.3.3",
    "draftjs-to-html": "^0.8.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "html-to-draftjs": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "lodash.debounce": "^4.0.8",
    "next": "^7.0.0",
    "next-with-apollo": "^3.1.3",
    "nprogress": "^0.2.0",
    "paypal-rest-sdk": "^1.8.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.8.3",
    "react-draft-wysiwyg": "^1.13.2",
    "react-markdown": "^4.0.6",
    "react-quill": "^1.3.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.5.0",
    "styled-components": "^3.4.9",
    "video-react": "^0.13.2",
    "waait": "^1.0.2"
  },
  "description": "Picus",
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "enzyme-to-json": "^3.3.4",
    "graphql-tools": "^4.0.0",
    "jest-transform-graphql": "^2.1.0",
    "jest": "^23.6.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "license": "ISC",
  "main": "index.js",
  "name": "picus-frontend",
  "scripts": {
    "build": "next build",
    "dev": "next -p 7777",
    "heroku-postbuild": "next build",
    "start": "next start",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "test": "NODE_ENV=test jest --watch"
  },
  "version": "1.0.0"
}
