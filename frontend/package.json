{
  "name": "project-name",
  "version": "1.0.0",
  "description": "The https://project-name.com web app",
  "keywords": [
    "react",
    "next.js",
    "web",
    "app",
    "www"
  ],
  "author": "PICUS <hello@picus.pt>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate:undo:all && cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate && cross-env NODE_ENV=test sequelize --seeders-path=./server/seeders db:seed:all && cross-env NODE_ENV=test testcafe chrome:headless tests/ --screenshots=./tests/screenshots/desktop --screenshots-on-fails",
    "test:desktop": "cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate:undo:all && cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate && cross-env NODE_ENV=test sequelize --seeders-path=./server/seeders db:seed:all && cross-env NODE_ENV=test testcafe chrome:headless tests/desktop --screenshots=./tests/screenshots/desktop --screenshots-on-fails",
    "test:mobile": "cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate:undo:all && cross-env NODE_ENV=test sequelize --migrations-path=./server/migrations db:migrate && cross-env NODE_ENV=test sequelize --seeders-path=./server/seeders db:seed:all && cross-env NODE_ENV=test testcafe \"chrome:headless:emulation:device=iphone 5\" tests/mobile --screenshots=./tests/screenshots/mobile --screenshots-on-fails",
    "db:migrate": "sequelize --migrations-path=./server/migrations db:migrate",
    "db:migrate:seeds": "sequelize --migrations-path=./server/migrations db:migrate && sequelize --seeders-path=./server/seeders db:seed:all",
    "dev": "cross-env NODE_ENV=development node index.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node index.js",
    "lint": "eslint --fix . && stylelint \"./client/**/*.js\"",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "analyze": "next build && cross-env NODE_ENV=production cross-env ANALYZE=1 node index.js",
    "docs": "node ./client/utils/docsGen.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "stylelint \"./client/**/*.js\"",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@zeit/next-source-maps": "0.0.3",
